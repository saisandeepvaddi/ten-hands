{
  "name": "ten-hands-app",
  "description": "One place to run your command-line tasks",
  "version": "2.10.4",
  "private": true,
  "homepage": "https://github.com/saisandeepvaddi/ten-hands",
  "author": {
    "name": "Sai Sandeep Vaddi",
    "email": "saisandeepvaddi@gmail.com",
    "url": "https://svaddi.dev"
  },
  "license": "MIT",
  "main": "./build/electron/index.js",
  "engines": {
    "node": ">=12.15.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "concurrently \"npm run build:watch\" && \"npm run electron-only\"",
    "start:server": "cross-env PORT=5010 node ./build/server/start",
    "dev": "cross-env NODE_ENV=developement ./node_modules/.bin/nodemon",
    "pack": "./node_modules/.bin/electron-builder --dir",
    "build:installers": "rm -rf ./dist && ./node_modules/.bin/electron-builder --publish never",
    "electron-only": "./node_modules/.bin/electron ./build/electron/index.js",
    "test:server": "jest --watch",
    "test:electron": "jest --testPathPattern=\".*/electron/__tests__/.*\\.test.ts\"",
    "lint": "eslint --ext .ts ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saisandeepvaddi/ten-hands"
  },
  "build": {
    "appId": "com.saisandeepvaddi.tenhands",
    "productName": "Ten Hands",
    "copyright": "Copyright Â© 2020 Sai Sandeep Vaddi",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "package.json"
    ],
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Utility"
    }
  },
  "dependencies": {
    "@sentry/electron": "^2.0.4",
    "@sentry/node": "^5.29.0",
    "@sentry/tracing": "^5.29.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "electron-is-dev": "^1.2.0",
    "electron-unhandled": "^3.0.2",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.1",
    "fix-path": "^3.0.0",
    "fkill": "^7.0.1",
    "git-repo-info": "^2.1.1",
    "just-enough-logger": "^1.0.4",
    "lowdb": "^1.0.0",
    "mkdirp": "^1.0.4",
    "morgan": "^1.10.0",
    "read-pkg": "^5.2.0",
    "serve-handler": "^6.1.3",
    "socket.io": "^3.0.4",
    "tree-kill": "^1.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/lowdb": "^1.0.9",
    "@types/morgan": "^1.9.2",
    "@types/node": "^12.20.15",
    "@types/serve-handler": "^6.1.0",
    "@types/socket.io": "^2.1.12",
    "@types/uuid": "^3.4.6",
    "electron-builder": "^22.9.1",
    "electron-react-devtools": "^0.5.3",
    "eslint-plugin-node": "^11.1.0",
    "jest-watch-yarn-workspaces": "^1.1.0",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "electron": "15.5.5"
  }
}
